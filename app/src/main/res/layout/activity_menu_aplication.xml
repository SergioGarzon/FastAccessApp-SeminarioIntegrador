<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundapp"
    tools:context=".MenuAplication">

    <Button
        android:id="@+id/btnLogoutResident"
        android:layout_width="232dp"
        android:layout_height="48dp"
        android:layout_marginBottom="16dp"
        android:background="@drawable/border_buttons"
        android:onClick="changeActivityPresentation"
        android:text="Logout"
        android:textColor="@color/white"
        android:textSize="25sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tableLayout5"
        app:layout_constraintVertical_bias="0.49" />

    <TextView
        android:id="@+id/txtFastAccessAppTitleResident"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/bomberescortcondital"
        android:foregroundTint="#00DB8C"
        android:text="FAST ACCESS APP"
        android:textColor="#0ea4af"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.125" />

    <TextView
        android:id="@+id/txtVisitorWelcomeMessage"
        android:layout_width="367dp"
        android:layout_height="47dp"
        android:foregroundTint="#00DB8C"
        android:text="Welcome to"
        android:textAlignment="center"
        android:textColor="#FFFFFF"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toTopOf="@+id/txtFastAccessAppTitleResident"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.641" />

    <TableLayout
        android:id="@+id/tableLayout5"
        android:layout_width="352dp"
        android:layout_height="469dp"
        android:layout_marginTop="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtFastAccessAppTitleResident"
        app:layout_constraintVertical_bias="0.085">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/profileaccount" />

            <Button
                android:id="@+id/btnProfilerAccountResident"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="ACCOUNT PROFILE"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView12"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/chataccount" />

            <Button
                android:id="@+id/btnChatResident"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="CHAT"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView15"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/sos" />

            <Button
                android:id="@+id/btnCallEmergencyResident"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="CALL EMERGENCY"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView23"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/notifications" />

            <Button
                android:id="@+id/button11"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="NOTIFICATIONS"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />
        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView24"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/visitorqrscan" />

            <Button
                android:id="@+id/btnRegisterVisitorResident"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="SCAN QR VISITOR"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView25"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/accesshistory" />

            <Button
                android:id="@+id/btnMenuHistory"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="ACCESS HISTORY"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/imageView26"
                android:layout_width="60dp"
                android:layout_height="60dp"
                app:srcCompat="@drawable/settingbutton" />

            <Button
                android:id="@+id/btnUserConfiguration"
                android:layout_width="290dp"
                android:layout_height="60dp"
                android:background="@drawable/border_buttons"
                android:text="GENERALS CONFIGURATIONS"
                android:textColor="@color/white"
                android:textSize="20sp"
                android:textStyle="bold|italic" />

        </TableRow>
    </TableLayout>


</androidx.constraintlayout.widget.ConstraintLayout>